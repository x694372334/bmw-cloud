<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bmw.app.dao.VersionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.bmw.app.model.Version">
        <id column="id" property="id" />
        <result column="network" property="network" />
        <result column="number" property="number" />
        <result column="grade" property="grade" />
        <result column="create_by" property="createBy" />
        <result column="create_time" property="createTime" />
        <result column="grade_by" property="gradeBy" />
        <result column="grade_time" property="gradeTime" />
        <result column="is_change" property="isChange" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, network, number, grade, create_by AS createBy, create_time AS createTime, grade_by AS gradeBy, grade_time AS gradeTime, is_change AS isChange
    </sql>

</mapper>
